
# -- Executables --
alias scheme='java -mx1g -cp bin org.goobs.scheme.Scheme'
alias pscheme='java -mx1g -cp bin org.goobs.scheme.Scheme --multithread'
alias server='java -mx250m -cp bin org.goobs.net.WebServer'

# -- Tests --
alias test_parser='java -mx2g -Xrunhprof:cpu=samples,depth=16 -cp etc/scala-library.jar:etc/scalatest.jar:dist/lib.jar:dist/lib-test.jar org.scalatest.tools.Runner -p . -o -s org.goobs.tests.CKYParserSpec'
alias test_search='java -mx2g -Xrunhprof:cpu=samples,depth=16 -cp etc/scala-library.jar:etc/scalatest.jar:dist/lib.jar:dist/lib-test.jar org.scalatest.tools.Runner -p . -o -s org.goobs.tests.SearchSpec'
